{
  "name": "oops-project",
  "version": "1.0.0",
  "description": "A project on react and webpack. It can be developed right away.",
  "scripts": {
    "clean": "rm -rf dist",
    "clean-dll": "rm -rf dll",
    "dll": "webpack --config=webpack/webpack.dll.config.js --mode development",
    "prod": "cross-env NODE_ENV=production webpack --color --config=webpack/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode=development --config=webpack/webpack.dev.config.js",
    "analysis": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.config.js --analysis",
    "start": "npm run dev",
    "build": "npm run dll && npm run prod",
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.prettierignore",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/prettier.js",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coyeah/oops-project.git"
  },
  "keywords": [
    "react",
    "antd",
    "webpack"
  ],
  "author": "Coyeah_chen@outlook.com",
  "license": "MIT",
  "homepage": "https://github.com/Coyeah/oops-project#readme",
  "bugs": {
    "url": "https://github.com/Coyeah/oops-project/issues"
  },
  "dependencies": {
    "antd": "^4.6.1",
    "classnames": "^2.2.6",
    "history": "^4.10.1",
    "hox": "^1.1.1",
    "lodash": "^4.17.20",
    "memoize-one": "^5.1.1",
    "path-to-regexp": "^6.1.0",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@babel/runtime-corejs3": "^7.11.2",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/classnames": "2.2.10",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.10.3",
    "@types/qs": "^6.9.4",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "5.1.4",
    "@types/webpack-env": "^1.15.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
